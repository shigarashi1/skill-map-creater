{
  "name": "skill-map-creater",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env REACT_APP_STAGING_FLAG=1 react-scripts build",
    "build:prod": "react-scripts build",
    "test": "react-scripts test --setupFiles ./src/configuration/setup-tests.ts --transformIgnorePatterns node_modules/(?!(ramda|date-fns)/)",
    "test:nowatch": "cross-env CI=true react-scripts test --setupFiles ./src/configuration/setup-tests.ts --transformIgnorePatterns node_modules/(?!(ramda|date-fns)/)",
    "eject": "react-scripts eject",
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "docs": "typedoc --out ./docs --target ES6 --mode file ./src/helpers ./src/types ./src/models ./src/services"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*": "prettier --write",
    "*.{css,scss}": "stylelint --fix",
    "*.{ts,tsx}": [
      "tslint -p ./tsconfig.json --fix",
      "git add"
    ]
  },
  "engines": {
    "node": "12.13.1",
    "npm": "6.12.1"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shigarashi1/skill-map-creater.git"
  },
  "author": "S.Igarashi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shigarashi1/skill-map-creater/issues"
  },
  "homepage": "https://github.com/shigarashi1/skill-map-creater#readme",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^1.3.11",
    "@material-ui/core": "^4.7.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/pickers": "^3.2.8",
    "@material-ui/styles": "^4.6.0",
    "@sentry/browser": "^5.9.1",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.14",
    "@types/react": "16.9.13",
    "@types/redux-sentry-middleware": "^0.1.1",
    "connected-react-router": "^6.6.1",
    "date-fns": "^2.8.1",
    "firebase": "^7.5.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.0",
    "notistack": "^0.9.6",
    "ramda": "^0.26.1",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.5",
    "react-dom": "^16.12.0",
    "react-number-format": "^4.3.1",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-test-renderer": "^16.12.0",
    "redux": "^4.0.4",
    "redux-observable": "^1.2.0",
    "redux-sentry-middleware": "^0.1.3",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.3",
    "source-map-explorer": "^2.1.2",
    "typescript": "3.7.2",
